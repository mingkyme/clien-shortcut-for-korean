{
  "name": "클리앙 한글 단축키",
  "version": "1.0",
  "description": "맥에서 한글상태의 단축키 기능을 활성화 합니다.",
  "author": "MINGKYME",
  "content_scripts": [
    {
      "matches": [
        "https://www.clien.net/*"
      ],
      "js": [
        "jq.js",
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://www.clien.net/*"
  ],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "manifest_version": 2
}